= form_for @cute do |f|
  - if @cute.errors.any?
    #error_explanation
      %h2= "#{pluralize(@cute.errors.count, "error")} prohibited this cute from being saved:"
      %ul
        - @cute.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :image_id
    = f.number_field :image_id
  .actions
    = f.submit 'Save'
